loot replace entity @s armor.head loot obs:technical/trial/elite_helmet
loot replace entity @s armor.chest loot obs:technical/trial/elite_chestplate
loot replace entity @s armor.legs loot obs:technical/trial/elite_pants
loot replace entity @s armor.feet loot obs:technical/trial/elite_shoes

loot replace entity @s weapon.mainhand loot obs:technical/trial/elite_weapon
loot replace entity @s[predicate=obs:chances/1] weapon.offhand loot obs:technical/trial/elite_shield

item modify entity @s weapon.mainhand obs:trial/enchant
item modify entity @s weapon.offhand obs:trial/enchant